# nvim ~/.config/neomutt/account.gmail

# My Mailboxes --------------------------------------------------------------------------
set folder = "~/.mail/gmail/"
set spoolfile = "+INBOX"
unmailboxes *
mailboxes +INBOX +FeelGood +Reimbursements +drafts +sent +archive
mailboxes "~/.cache/mutt_results"
set mbox  = "+archive"
set postponed = "+drafts"

unset from
unset sendmail
set from = "rylee.alanza@gmail.com"
set sendmail = "/usr/bin/msmtp -a personal"
